<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    package="com.example.root.sqliteproject">    <permission android:name="com.example.root.sqliteproject.READ_DATABASE" android:protectionLevel="normal"/>    <permission android:name="com.example.root.sqliteproject.WRITE_DATABASE" android:protectionLevel="normal"/>    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity android:name=".MainActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".FormAddUpdateActivity"/>        <provider            android:authorities="com.example.root.sqliteproject"            android:name=".provider.NoteProvider"            android:exported="true"            android:readPermission="com.example.root.sqliteproject.READ_DATABASE"            android:writePermission="com.example.root.sqliteproject.WRITE_DATABASE" />    </application></manifest>